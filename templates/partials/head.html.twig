<!DOCTYPE html>
<html lang="de">
    <head>
		{% block head %}
		{% include 'partials/metadata.html.twig' %}
		<meta charset="utf-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <meta name="viewport" content="width=device-width, initial-scale=1">
      {% if header.robots %}
      <meta name="robots" content="{{ header.robots }}">
      {% endif %}
      <meta name="theme-color" content="#343434">
      <title>{% if header.title %}{{ header.title }} | {% endif %}{{ site.title }}</title>
      <link href="{{ theme_url }}/assets/css/bootstrap.min.css" rel="stylesheet" media="screen">
      <link href="{{ theme_url }}/assets/css/print.min.css" rel="stylesheet" media="print">
      <link href="{{ theme_url }}/assets/css/font-awesome.min.css" rel="stylesheet" media="screen">
      <link href="{{ theme_url }}/assets/styles/monokai_sublime.css" rel="stylesheet" media="screen">
      <link href="{{ theme_url }}/assets/css/bootstrap-custom.css" rel="stylesheet" media="screen">

      <link rel="shortcut icon" href="{{ theme_url }}/assets/images/layout/favicon.ico" type="image/x-icon" />
		  <link rel="icon" href="{{ theme_url }}/assets/images/layout/favicon.ico" type="image/x-icon" />

      <link rel="apple-touch-icon" href="{{ theme_url }}/assets/images/touch-images/apple-touch-icon.png" />
    	<link rel="apple-touch-icon" sizes="57x57" href="{{ theme_url }}/assets/images/touch-images/apple-touch-icon-57x57.png" />
    	<link rel="apple-touch-icon" sizes="60x60" href="{{ theme_url }}/assets/images/touch-images/apple-touch-icon-60x60.png" />
    	<link rel="apple-touch-icon" sizes="72x72" href="{{ theme_url }}/assets/images/touch-images/apple-touch-icon-72x72.png" />
    	<link rel="apple-touch-icon" sizes="76x76" href="{{ theme_url }}/assets/images/touch-images/apple-touch-icon-76x76.png" />
    	<link rel="apple-touch-icon" sizes="114x114" href="{{ theme_url }}/assets/images/touch-images/apple-touch-icon-114x114.png" />
    	<link rel="apple-touch-icon" sizes="120x120" href="{{ theme_url }}/assets/images/touch-images/apple-touch-icon-120x120.png" />
    	<link rel="apple-touch-icon" sizes="144x144" href="{{ theme_url }}/assets/images/touch-images/apple-touch-icon-144x144.png" />
    	<link rel="apple-touch-icon" sizes="152x152" href="{{ theme_url }}/assets/images/touch-images/apple-touch-icon-152x152.png" />

      <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
      <![endif]-->
    {% endblock head%}
    </head>